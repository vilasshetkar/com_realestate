<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="3.1.0" client="site" method="upgrade">
    <name>Home Project!</name>
    <author>Vilas Shetkar</author>
    <version>1.1.0</version>
    <description>Home Project! module.</description>
    <files>
        <filename>mod_homeproject.xml</filename>
        <filename module="mod_featuredproj">mod_homeproject.php</filename>
        <filename>index.html</filename>
        <filename>helper.php</filename>
        <filename>tmpl/default.php</filename>
        <filename>tmpl/list.php</filename>
        <filename>tmpl/index.html</filename>
    </files>
    <config>
	<fields name="params">
        <fieldset addfieldpath="/components/com_realestate/models/fields" name="home_project_setting" label="Home Project Setting">
			
			<field name="title" type="City" label="JGLOBAL_TITLE"
		        description="JFIELD_TITLE_DESC"
		        required="true" />
			
            <field name="home_project" type="text" default=""
                label="Project Name" />

        </fieldset>
    </fields>
	</config>
</extension>